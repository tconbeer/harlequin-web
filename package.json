{
  "name": "harlequin-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-organize-imports --plugin prettier-plugin-tailwindcss .",
    "themes": "svgo -f ./src/lib/assets/themes -o ./src/lib/assets/themes"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@fastify/pre-commit": "^2.0.2",
    "@sveltejs/adapter-vercel": "^5.1.1",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.33.0",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rehype-slug": "^6.0.0",
    "svelte": "^5",
    "svelte-check": "^3.5.1",
    "svgo": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.4.20"
  },
  "type": "module"
}
